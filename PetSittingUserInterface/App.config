<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <connectionStrings>
    <add name="PetSitting.Connection"
         connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=PetSitting;Integrated Security=True;"
         providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="PetSittingConnection" value="PetSitting.Connection" />
    <add key="EnableLogging" value="True" />
    <add key="LoggingPath" value="C:\LoggingData" />
  </appSettings>
</configuration>